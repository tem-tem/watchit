<div class="row justify-content-center", id="flash">
  <div class="col-md-4 mt-5 position-absolute">
      <div class="alert alert-<%= key %> alert-dismissible fade show bring-to-front" role="alert">
        <%= message %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
  </div>
</div>
